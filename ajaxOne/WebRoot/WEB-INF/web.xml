<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>ZyServlet</servlet-name>
    <servlet-class>servlet.ZyServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ShiServlet</servlet-name>
    <servlet-class>servlet.ShiServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>QuServlet</servlet-name>
    <servlet-class>servlet.QuServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ShengServlet</servlet-name>
    <servlet-class>servlet.ShengServlet</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>ZyServlet</servlet-name>
    <url-pattern>/ZyServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShiServlet</servlet-name>
    <url-pattern>/ShiServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QuServlet</servlet-name>
    <url-pattern>/QuServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShengServlet</servlet-name>
    <url-pattern>/ShengServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
